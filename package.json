{
  "name": "peli-gen-visual-testing",
  "version": "1.0.0",
  "description": "Visual Regression Testing for Pelican Static Blog",
  "private": true,
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug",
    "update": "playwright test --update-snapshots",
    "report": "playwright show-report",
    "install:browsers": "playwright install chromium webkit",
    "install:browsers:with-deps": "playwright install --with-deps chromium webkit"
    ,"install:system-deps:fedora": "bash ./scripts/install_playwright_fedora.sh"
  },
  "keywords": [
    "playwright",
    "visual-regression",
    "testing"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@playwright/test": "1.48.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
